{
    "suggest.enablePreview": true,
    "suggest.echodocSupport": true,
    "suggest.floatEnable": true,
    "suggest.autoTrigger": "trigger",
    "suggest.noselect": true,
    "suggest.completionItemKindLabels": {
        "class": "",
        "color": "",
        "constant": "",
        "default": "",
        "enum": "",
        "enumMember": "",
        "file": "",
        "folder": "",
        "field": "",
        "function": "",
        "interface": "",
        "keyword": "",
        "method": "",
        "module": "",
        "operator": "",
        "reference": "",
        "snippet": "",
        "struct": "",
        "text": "",
        "typeParameter": "",
        "unit": "",
        "value": "",
        "variable": "",
        "event": "",
        "property": ""
    },
    "suggest.localityBonus": true,
    "suggest.detailField": "preview",
    "suggest.fixInsertedWord": true,
    "suggest.snippetIndicator": "",
    "diagnostic.infoSign": "",
    "diagnostic.hintSign": "",
    "diagnostic.warningSign": "",
    "diagnostic.errorSign": "",
    "diagnostic.highlightOffset": 1000,
    "diagnostic.virtualText": true,
    "diagnostic.checkCurrentLine": true,
    "python.jediEnabled": false,
    "python.analysis.typeCheckingMode": "off",
    "codeLens.enable": true,
    "signature.target": "float",
    "coc.preferences.hoverTarget": "float",
    "coc.preferences.snippetStatusText": "SNIP",
    "coc.preferences.useQuickfixForLocations": true,
    "coc.source.vimtex.priority": 100,
    "coc.source.buffer.enable": false,
    "coc.source.around.enable": true,
    "snippets.priority": 30,
    "Lua.diagnostics.globals": ["vim"],
    "git.realtimeGutters": true,
    "git.addedSign.text": "▎",
    "git.changedSign.text": "▎",
    "git.removedSign.text": "◢",
    "git.topRemovedSign.text": "◥",
    "git.changeRemovedSign.text": "◢",
    "git.addedSign.hlGroup": "DiffAddGutter",
    "git.changedSign.hlGroup": "DiffChangeGutter",
    "git.removedSign.hlGroup": "DiffDeleteGutter",
    "git.topRemovedSign.hlGroup": "DiffDeleteGutter",
    "git.changeRemovedSign.hlGroup": "DiffChangeGutter",
    "explorer.icon.enableNerdfont": true,
    "explorer.width": 25,
    "metals.statusBarEnabled": true,
    "explorer.file.column.git.showIgnored": true,
    "explorer.keyMappingMode": "none",
    "explorer.keyMappings": {
        "*": "toggleSelection",
        "<tab>": "actionMenu",
        "h": "shrink",
        "l": "expand",
        "J": [
            "toggleSelection",
            "normal:j"
        ],
        "K": [
            "toggleSelection",
            "normal:k"
        ],
        "gl": "expandRecursive",
        "gh": "shrinkRecursive",
        "o": "expandOrShrink",
        "e": "open",
        "E": "openInVsplit",
        "t": "openInTab",
        "<bs>": "gotoParent",
        "y": "copyFilepath",
        "Y": "copyFilename",
        "c": "copyFile",
        "x": "cutFile",
        "p": "pasteFile",
        "d": "delete",
        "D": "deleteForever",
        "a": "addFile",
        "A": "addDirectory",
        "r": "rename",
        ".": "toggleHidden",
        "R": "refresh",
        "?": "help",
        "q": "quit",
        "X": "systemExecute",
        "gd": "listDrive",
        "f": "search",
        "F": "searchRecursive",
        "gf": "gotoSource:file",
        "gb": "gotoSource:buffer",
        "[[": "sourcePrev",
        "]]": "sourceNext",
        "[d": "diagnosticPrev",
        "]d": "diagnosticNext",
        "[c": "gitPrev",
        "]c": "gitNext",
        "<<": "gitStage",
        ">>": "gitUnstage"
    },
    "list.normalMappings": {
        "<C-c>": "do:exit"
    },
    "list.insertMappings": {
        "<C-c>": "do:exit"
    },
    "go.enable": true,
    "go.goplsOptions": {
        "deepCompletion": true,
        "completeUnimported": true,
        "staticcheck": true
    },
    "languageserver": {
        "ocaml": {
            "command": "ocaml-language-server",
            "args": [
                "--stdio"
            ],
            "filetypes": [
                "ocaml",
                "reason"
            ]
        },
        "swift": {
            "command": "sourcekit-lsp",
            "filetypes": [
                "swift"
            ]
        }
    }
}
