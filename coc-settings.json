{
    "suggest.enablePreview": true,
    "suggest.echodocSupport": true,
    "suggest.floatEnable": true,
    "suggest.autoTrigger": "trigger",
    "suggest.noselect": true,
    "suggest.completionItemKindLabels": {
        "class":         "",
        "color":         "",
        "constant":      "",
        "default":       "",
        "enum":          "",
        "enumMember":    "",
        "file":          "",
        "folder":        "",
        "field":         "",
        "function":      "",
        "interface":     "",
        "keyword":       "",
        "method":        "",
        "module":        "",
        "operator":      "",
        "reference":     "",
        "snippet":       "",
        "struct":        "",
        "text":          "",
        "typeParameter": "",
        "unit":          "",
        "value":         "",
        "variable":      "",
        "event":         "",
        "property":      ""
    },
    "suggest.localityBonus": true,
    "suggest.detailField": "preview",
    "suggest.fixInsertedWord": true,
    "suggest.snippetIndicator": "",
    "suggest.enablePreselect": true,
    "suggest.removeDuplicateItems": true,
    "suggest.disableMenu": false,
    "suggest.disableMenuShortcut": false,
    "suggest.lowPrioritySourceLimit": 5,
    "suggest.highPrioritySourceLimit": 8,
    "tabnine.priority": 80,
    "rust-analyzer.lens.run": true,
    "diagnostic.infoSign":    "",
    "diagnostic.hintSign":    "",
    "diagnostic.warningSign": "",
    "diagnostic.errorSign":   "",
    "diagnostic.highlightOffset": 1000,
    "diagnostic.virtualText": true,
    "diagnostic.checkCurrentLine": true,
    "diagnostic.signPriority": 10,
    "python.jediEnabled": false,
    "python.analysis.typeCheckingMode": "off",
    "codeLens.enable": true,
    "signature.target": "float",
    "coc.preferences.hoverTarget": "float",
    "coc.preferences.snippetStatusText": "SNIP",
    "coc.preferences.useQuickfixForLocations": true,
    "coc.source.vimtex.priority": 100,
    "coc.source.buffer.enable": false,
    "coc.source.around.enable": true,
    "snippets.priority": 95,
    "Lua.diagnostics.globals": ["vim"],
    "metals.statusBarEnabled": true,
    "list.normalMappings": {
        "<C-c>": "do:exit"
    },
    "list.insertMappings": {
        "<C-c>": "do:exit"
    },
    "go.enable": true,
    "go.goplsOptions": {
        "deepCompletion": true,
        "completeUnimported": true,
        "staticcheck": true
    },
    "languageserver": {
        "ocaml": {
            "command": "ocaml-language-server",
            "args": [
                "--stdio"
            ],
            "filetypes": [
                "ocaml",
                "reason"
            ]
        },
        "swift": {
            "command": "sourcekit-lsp",
            "filetypes": [
                "swift"
            ]
        }
    }
}
