    "git.changeRemovedSign.hlGroup": "DiffChangeGutter",
    "explorer.icon.enableNerdfont": true,
    "explorer.width": 25,
    "metals.statusBarEnabled": true,
    "coc.source.vimtex.priority": 100,
    "coc.source.buffer.enable": false,
    "coc.source.tag.enable": false,
    "coc.source.around.enable": false,
    "explorer.file.column.git.showIgnored": true,
    "snippets.priority": 30,
    "Lua.diagnostics.globals": ["vim"],
    "explorer.keyMappingMode": "none",
    "explorer.keyMappings": {
        "*": "toggleSelection",
        "<tab>": "actionMenu",
        "h": "shrink",
        "l": "expand",
        "J": [
            "toggleSelection",
            "normal:j"
        ],
        "K": [
            "toggleSelection",
            "normal:k"
        ],
        "gl": "expandRecursive",
        "gh": "shrinkRecursive",
        "o": "expandOrShrink",
        "e": "open",
        "E": "openInVsplit",
        "t": "openInTab",
        "<bs>": "gotoParent",
        "y": "copyFilepath",
        "Y": "copyFilename",
        "c": "copyFile",
        "x": "cutFile",
        "p": "pasteFile",
        "d": "delete",
        "D": "deleteForever",
        "a": "addFile",
        "A": "addDirectory",
        "r": "rename",
        ".": "toggleHidden",
        "R": "refresh",
        "?": "help",
        "q": "quit",
        "X": "systemExecute",
        "gd": "listDrive",
        "f": "search",
        "F": "searchRecursive",
        "gf": "gotoSource:file",
        "gb": "gotoSource:buffer",
        "[[": "sourcePrev",
        "]]": "sourceNext",
        "[d": "diagnosticPrev",
        "]d": "diagnosticNext",
        "[c": "gitPrev",
        "]c": "gitNext",
        "<<": "gitStage",
        ">>": "gitUnstage"
    },
    "list.normalMappings": {
        "<C-c>": "do:exit"
    },
    "list.insertMappings": {
        "<C-c>": "do:exit"
    },
    "go.enable": true,
    "go.goplsOptions": {
        "deepCompletion": true,
        "completeUnimported": true,
        "staticcheck": true
    },
    "languageserver": {
        "ocaml": {
            "command": "ocaml-language-server",
            "args": [
                "--stdio"
            ],
            "filetypes": [
                "ocaml",
                "reason"
            ]
        },
        "swift": {
            "command": "sourcekit-lsp",
            "filetypes": [
                "swift"
            ]
        }
    }
}
